    "from itertools import permutations\n",
    "\n",
    "distances = [[3, 6], [2, 3]]\n",
    "\n",
    "output = [[row[y] for row, y in zip(distances, permutation)]\n",
    "          for permutation in permutations(range(len(distances)))]\n",
    "\n",
    "print(output)"
   ]
